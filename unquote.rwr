unquote {
  Main [items+] = ‛«items»’
  Item_quotedlist [sq list] = ‛(❲quote❳ «list»)’
  Item_quotedatom [sq atom] = ‛(❲quote❳ «atom»)’
  Item_other [c] = ‛«c»’

  List [lb xs+ rb] = ‛«lb»«xs»«rb»’
  ListOrAtom [x] = ‛«x»’
  Atom [x] = ‛«x»’
  sym [lb uptorb] = ‛«lb»«uptorb»’
  comment [lb uptorb] = ‛«lb»«uptorb»’
  number [ds+] = ‛«ds»’
  string [dq uptodq] = ‛«dq»«uptodq»’
  bq [c] = ‛«c»’
  eq [c] = ‛«c»’
  sq [c] = ‛«c»’
  upto [cs* right] = ‛«cs»«right»’
}
